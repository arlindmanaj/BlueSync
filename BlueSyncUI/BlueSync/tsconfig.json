{
    "compilerOptions": {
      "allowJs": true,
      "target": "esnext",
      "module": "esnext",
      "jsx": "react-native",
      "strict": true,
      "moduleResolution": "node",
      "baseUrl": "./",
      "paths": {
        "*": ["node_modules/*"]
      },
      "resolveJsonModule": true,
      "skipLibCheck": true
    },
    "exclude": ["node_modules", "babel.config.js", "metro.config.js"]
  }
  